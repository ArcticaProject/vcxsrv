    // begin modifier mappings
    modifier_map Shift  { Shift_R };
    modifier_map Mod1	{ Meta_L, Meta_R };
    modifier_map Mod2	{ Alt_R };
    modifier_map Mod3	{ Num_Lock };
    modifier_map Mod4	{ Alt_L };
};

