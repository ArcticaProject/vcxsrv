default  partial alphanumeric_keys
xkb_symbols "basic" {

    name[Group1]="Bosnian";

    include "rs(latin)"
};

partial alphanumeric_keys
xkb_symbols "us" {

    name[Group1]= "Bosnian (US keyboard with Bosnian letters)";

    include "rs(latinyz)"
};


