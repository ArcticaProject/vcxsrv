    alias <CAPS> = <AC00>;
    <AC01> = 84;
    <AC02> = 85;
    <AC03> = 86;
    <AC04> = 87;
    <AC05> = 88;
    <AC06> = 89;
    <AC07> = 90;
    <AC08> = 91;
    <AC09> = 92;
    <AC10> = 93;
    <AC11> = 94;
    <AC12> = 95;
    alias <BKSL> = <AC12>;
    <RTRN> = 96;
    //
    <KP4>  = 98;
    <KP5>  = 99;
    <KP6>  = 100;
    // End Row C

    // Row B
    <OPEN> = 79;
    <PAST> = 80;
    //
    <LFSH> = 106;
    <AB00> = 131;
    alias <LSGT> = <AB00>;
    <AB01> = 107;
    <AB02> = 108;
    <AB03> = 109;
    <AB04> = 110;
