    <AB07> = 113;
    <AB08> = 114;
    <AB09> = 115;
    <AB10> = 116;
    <RTSH> = 117;
    <LNFD> = 118;
    //
    <KP1>  = 119;
    <KP2>  = 120;
    <KP3>  = 121;
    <KPEN> = 97;
    // End Row B

    // Row A
    <HELP> = 125;
    //
    <AA00> = 126;
    //    alias <LCTL> = <AA00>;
    <LALT> = 26;
    <LMTA> = 127;
    <SPCE> = 128;
    <RMTA> = 129;
    <COMP> = 74;
    <ALGR> = 20;
    alias <RALT> = <ALGR>;
    //
    <KP0>  = 101;
    <KPDL> = 57;
    // End Row A

    indicator 4 = "Caps Lock";
    indicator 3 = "Compose";
    indicator 2 = "Scroll Lock";
    indicator 1 = "Num Lock";
};

xkb_keycodes "type4_se" {

    include "sun(type4__se)"

    alias <LCTL> = <AA00>;
    alias <CAPS> = <AC00>;
};

xkb_keycodes "type4_se_swapctl" {

    include "sun(type4__se)"

    alias <LCTL> = <AC00>;
    alias <CAPS> = <AA00>;
};
