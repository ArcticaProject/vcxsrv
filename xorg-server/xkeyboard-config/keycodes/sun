    <KPDV> = 53;
    <KPMU> = 54;
    <KPSU> = 78;
    //End Row E

    // Row D
    <PROP> = 32;
    <UNDO> = 33;
    //
    <AD00> = 60;
    alias <TAB> = <AD00>;
    <AD01> = 61;
    <AD02> = 62;
    <AD03> = 63;
    <AD04> = 64;
    <AD05> = 65;
